<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:converters="clr-namespace:Common.Converters">

    <ColorConverter x:Key="EnumToColor" />

    <converters:BoolToVisibilityConverter x:Key="BoolToVisHidden" Reverse="False" Hide="true" />
    <converters:BoolToVisibilityConverter x:Key="BoolToVis" Reverse="False" />
    <converters:BoolToVisibilityConverter x:Key="InvBoolToVis" Reverse="True"/>
    <converters:BoolToVisibilityConverter x:Key="InvBoolToVisHidden" Reverse="True" Hide="True"/>
    <converters:ColorToSolidColorBrushValueConverter x:Key="ColorToBrush" />
    <converters:IntToSolidColorBrushValueConverter x:Key="IntToBrush" />
    <converters:IntToSolidColorBrushValueConverter x:Key="BrushToInt"  Reverse="True" />
    <converters:InverseBoolConverter x:Key="InvBool"></converters:InverseBoolConverter>
    <converters:NullToBooleanConverter x:Key="NullToBool" Reverse="False"/>
    <converters:NullToBooleanConverter x:Key="InvNullToBool" Reverse="True"/>
    <converters:NullToVisibilityConverter x:Key="NullToVis" Reverse="False"/>
    <converters:NullToVisibilityConverter x:Key="InvNullToVis" Reverse="True"/>
    <converters:NullToVisibilityConverter x:Key="NullToVisHidden" Reverse="False" Hide="True" />
    <converters:NullToVisibilityConverter x:Key="InvNullToVisHidden" Reverse="True" Hide="True"/>

</ResourceDictionary>